# Configuration Backend - Millésime Sans Frontières
# Copiez ce fichier en .env et modifiez les valeurs

# Mode développement
DEBUG=true

# Base de données
DATABASE_URL=postgresql://millesime_user:millesime_password@localhost:5432/millesime_db

# Redis
REDIS_URL=redis://localhost:6379

# Sécurité
SECRET_KEY=millesime-secret-key-change-in-production-very-long-and-complex

# CORS
ALLOWED_HOSTS=["http://localhost:3000", "http://localhost:8080", "http://127.0.0.1:3000", "http://127.0.0.1:8080"]

# Logging
LOG_LEVEL=INFO
